alias fbtokenomp='EMAIL=jashment@owletcare.com && PASSWORD="passwo$

# The next line updates PATH for the Google Cloud SDK.
if [ -f '/Users/jashment/Downloads/google-cloud-sdk/path.zsh.inc' $

